{
  "name": "conflict-resolver-service",
  "version": "0.9.3",
  "description": "The Conflict-Resolver Service (CSR) can be queried to provide a signed resolution about the non-repudiation protocol associated to an invoice being valid or invalid. It could be invoked by either the consumer or the provider.",
  "author": {
    "name": "Juan Hern√°ndez Serrano",
    "email": "j.hernandez@upc.edu",
    "url": "https://github.com/juanelas"
  },
  "license": "EUPL-1.2",
  "repository": "gitlab:i3-market/code/wp3/t3.2/conflict-resolution/conflict-resolver-service",
  "keywords": [
    "conflict resolution",
    "conflict",
    "non-repudiation",
    "i3-MARKET",
    "i3m"
  ],
  "main": "./dist/index.js",
  "directories": {
    "build": "./build",
    "dist": "./dist",
    "types": "./types",
    "spec": "./dist/spec"
  },
  "exports": {
    ".": "./dist/spec/openapi.json",
    "./openapi.yaml": "./dist/spec/openapi.yaml",
    "./openapi.json": "./dist/spec/openapi.json",
    "./types": "./types/openapi.d.ts"
  },
  "scripts": {
    "openapi": "run-s openapi:json openapi:types",
    "openapi:json": "ts-node build/createOASJsonBundle.ts",
    "openapi:types": "ts-node build/openApiTypes.ts",
    "tsc": "tsc -p tsconfig.build.json",
    "build": "run-s clean openapi tsc",
    "start": "node dist/index.js",
    "predev": "npm run openapi",
    "dev": "ts-node index.ts",
    "clean": "rimraf dist",
    "test": "mocha -r ts-node/register 'test/**/*.ts'"
  },
  "ts-standard": {
    "project": "./tsconfig.json",
    "ignore": [
      "dist/**/*",
      "types/**/*"
    ]
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.0.3",
    "@dtsgenerator/replace-namespace": "^1.5.1",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^16.11.12",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.4",
    "@types/swagger-ui-express": "^4.1.3",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "dtsgenerator": "^3.13.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mocha": "^9.1.3",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "openapi-types": "^10.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "ts-standard": "^11.0.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@i3m/non-repudiation-library": "^1.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.13.4",
    "express-session": "^1.17.1",
    "jose": "^4.3.7",
    "jsonwebtoken": "^8.5.1",
    "openid-client": "^5.1.0",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "swagger-ui-express": "^4.2.0"
  }
}
